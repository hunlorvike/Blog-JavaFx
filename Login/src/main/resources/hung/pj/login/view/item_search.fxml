                <Insets bottom="5.0" left="10.0" right="5.0" top="5.0"/>
            </padding>

            <Label fx:id="labelTopBox1" text="Dữ liệu tên"/>
            <Label fx:id="labelBotBox1" text="Dữ liệu role" styleClass="b"/>
        </VBox>
        <VBox prefHeight="50.0" prefWidth="250.0">
            <padding>
                <Insets bottom="5.0" left="10.0" right="5.0" top="5.0"/>
            </padding>

            <Label fx:id="labelBox2" text="Dữ liệu email"/>
        </VBox>
        <VBox prefHeight="200.0" prefWidth="180.0">
            <padding>
                <Insets bottom="5.0" left="10.0" right="5.0" top="5.0"/>
            </padding>

            <Label fx:id="labelBox3" text="Dữ liệu ngày khởi tạo"/>
        </VBox>
    </HBox>

</AnchorPane>
